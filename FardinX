Reg.exe add "HKLM\SYSTEM\CurrentControlSet\Services\Tcpip\Parameters" /v "TcpTimedWaitDelay" /t REG_DWORD /d "30" /f
Reg.exe add "HKLM\SYSTEM\CurrentControlSet\Services\Tcpip\Parameters" /v "StrictTimeWaitSeqCheck" /t REG_DWORD /d "1" /f
REM ;"NumTcbTablePartitions"=dword:00000008
REM ;This parameter controls the number of TCP control block (TCB ) table partitions.
REM ;The TCB table can be partitioned to improve scalability on multiprocessor systems by reducing contention on the TCB table.
REM ;The value should be a power of two, that is, 2, 4, 8, 16, 32, and so on.
REM ;On multiprocessor systems, change the number of partitions to four times the number of processors in your system.
REM ;[HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Services\Tcpip\Parameters\Interfaces\************]
REM ;If only acking data and not any control packets, ack once every * packets, instead of the default of two.
REM ;This helps reduce packet processing costs for the Network Stack in the case of large writes (uploads) from the client into the server.
REM ;For each adapter, add:
REM ;for Gigabit
REM ;"TcpAckFrequency"=dword:00000013
REM ;for FastEthernet
REM ;"TcpAckFrequency"=dword:00000005
Reg.exe add "HKLM\SYSTEM\CurrentControlSet\Enum\USB\VID_054C&PID_0CE6&MI_03\6&286278dc&1&0003\Device Parameters" /v "SelectiveSuspendEnabled" /t REG_BINARY /d "00" /f
Reg.exe add "HKLM\SYSTEM\CurrentControlSet\Enum\USB\VID_054C&PID_0CE6&MI_03\6&286278dc&1&0003\Device Parameters" /v "EnhancedPowerManagementEnabled" /t REG_DWORD /d "0" /f
Reg.exe add "HKLM\SYSTEM\CurrentControlSet\Enum\USB\VID_054C&PID_0CE6&MI_03\6&286278dc&1&0003\Device Parameters" /v "AllowIdleIrpInD3" /t REG_DWORD /d "1" /f
Reg.exe add "HKLM\SYSTEM\CurrentControlSet\Enum\USB\VID_054C&PID_0CE6&MI_03\6&286278dc&1&0003\Device Parameters" /v "LegacyTouchScaling" /t REG_DWORD /d "0" /f
Reg.exe add "HKLM\SYSTEM\CurrentControlSet\Enum\USB\VID_054C&PID_0CE6&MI_03\6&286278dc&1&0003\Device Parameters" /v "WriteReportExSupported" /t REG_DWORD /d "1" /f
Reg.exe add "HKLM\SYSTEM\CurrentControlSet\Enum\USB\VID_054C&PID_0CE6&MI_03\6&286278dc&1&0003\Device Parameters" /v "DeviceResetNotificationEnabled" /t REG_DWORD /d "1" /f
Reg.exe add "HKLM\SYSTEM\CurrentControlSet\Enum\USB\VID_054C&PID_0CE6&MI_03\6&286278dc&1&0003\Device Parameters" /v "VendorRevision" /t REG_DWORD /d "0" /f
Reg.exe add "HKLM\SYSTEM\CurrentControlSet\Enum\USB\VID_054C&PID_0CE6&MI_03\6&286278dc&1&0003\Device Parameters" /v "RevisionId" /t REG_DWORD /d "256" /f
Reg.exe add "HKLM\SYSTEM\CurrentControlSet\Enum\USB\VID_054C&PID_0CE6&MI_03\6&286278dc&1&0003\Device Parameters" /v "ExtPropDescSemaphore" /t REG_DWORD /d "1" /f
Reg.exe add "HKLM\SYSTEM\CurrentControlSet\Enum\USB\VID_054C&PID_0CE6&MI_03\6&286278dc&1&0003\Device Parameters" /v "DeviceSelectiveSuspended" /t REG_DWORD /d "0" /f
Reg.exe add "HKLM\SYSTEM\CurrentControlSet\Enum\USB\VID_054C&PID_0CE6&MI_03\6&286278dc&1&0003\Device Parameters\e5b3b5ac-9725-4f78-963f-03dfb1d828c7" /f

goto M9



