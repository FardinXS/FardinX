)
if %ans%==8 (
goto M15-T8
)
if %ans%==9 (
goto M15-T9
)
if %ans%==10 (
goto M15-T10
)
if %ans%==m (
goto main menu
)


:M15-T1
sfc/scannow
pause
goto M15

:M15-T2
DISM.exe /Online /Cleanup-image /Restorehealth
goto M15


:M15-T3
Dism /Online /Cleanup-Image /RestoreHealth /Source:wim:J:\sources\install.wim:1 /limitaccess
goto M15


:M15-T4
DISM.exe /Online /Cleanup-image /Checkhealth
goto M15

:M15-T5
